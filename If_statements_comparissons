def max(num1, num2, num3):
    if num1>num2 and num1>num3:
        print("The maximum number is: ",num1)
    elif num2>num1 and num2>num3:
        print("The maximum number is: ",num2)
    else:
        print("The maximum number is: ",num3) 

max(10,11,12) 
  
            
 